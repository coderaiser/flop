    "win32": "^1.0.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.14.1",
    "nyc": "^10.1.2",
    "redrun": "^5.9.5",
    "tape": "^4.6.3"
