{
  "name": "flop",
  "version": "2.0.5",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "FLOP - simple FoLder OPerations",
  "homepage": "http://github.com/coderaiser/flop",
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/flop.git"
  },
  "main": "lib/flop.js",
  "bin": {
    "flop": "legacy/bin/flop.js",
    "flop_": "bin/flop.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "6to5:lib": "babel lib -d legacy/lib",
    "6to5:bin": "babel bin -d legacy/bin",
    "build": "redrun 6to5:* legacy:*",
    "wisdom": "npm run build",
    "lint": "redrun lint:*",
    "lint:eslint": "eslint lib test",
    "lint:eslint:bin": "eslint --rule 'no-console:0' bin",
    "legacy:index": "echo \"module.exports = require('./lib/flop');\" > legacy/index.js",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc npm test",
    "test": "tape test/*.js",
    "watch:coverage": "npm run watcher -- npm run coverage",
    "watch:test": "npm run watcher -- npm test",
    "watcher": "nodemon -w test -w lib --exec"
  },
  "dependencies": {
    "checkup": "^1.3.0",
    "copymitter": "^2.0.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.0",
    "readify": "^4.0.0",
    "remy": "^2.0.0",
    "timem": "^1.1.0",
    "trammel": "^1.0.0",
    "win32": "^1.0.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "chalk": "^1.1.3",
    "coveralls": "^2.11.16",
    "eslint": "^4.0.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.2",
    "redrun": "^5.9.5",
    "sinon": "^2.3.4",
    "tape": "^4.6.3"
  }
}